{
  "name": "Snippet Extension",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Snippet Extension"
  },
  "content_scripts": [
    {
        "matches": ["https://stackoverflow.com/questions/*"],
        "js": ["fetch_snippets.js"]
    }
  ], 
  "version": "1.0"
}